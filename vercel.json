{
    "version": 2,
    "name": "vpn-comparateur",
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "server.js"
        },
        {
            "src": "/(.+)",
            "dest": "server.js"
        }
    ]
}